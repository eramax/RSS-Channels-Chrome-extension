{
    "manifest_version": 2,
    "name": "Youtube RSS",
    "description": "subscribe to youtube channels by rss",
    "version": "1.0.0",
    "icons": {
        "128": "icon128.png"
    },
    "background": {
        "scripts": ["bg.js"]
    },
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_icon": "icon19.png",
        "default_title": ""
    },
    "permissions": ["activeTab"]
}